# GitHub Personal Access Token
# Get one from: https://github.com/settings/tokens
# Required scope: 'repo'
GITHUB_TOKEN=your_github_token_here

# iCloud Configuration (optional - can also use config.yaml)
# ICLOUD_APPLE_ID=your-apple-id@example.com
# ICLOUD_PASSWORD=your_password_here
# ICLOUD_2FA_CODE=optional_2fa_code
# ICLOUD_2FA_DEVICE_ID=optional_device_id

# Google Drive Configuration (optional)
# GOOGLE_DRIVE_CREDENTIALS_FILE=credentials.json
